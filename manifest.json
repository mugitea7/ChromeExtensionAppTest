{
    "name": "GoogleExtensionAppSample",
    "version": "1.1.8",
    "manifest_version": 2,
    "description": "GoogleExtensionAppSample",
    "options_ui": {
        "page": "option/options.html",
        "chrome_style": true
    },
    "permissions": ["contextMenus"],
    "background": {
        "scripts": ["lib/jquery-3.5.1.min.js","src/background.js"]
    },
    "browser_action": {
        "default_title": "Sample",
        "default_popup": "popup/popup.html"
    }
}